   		<typeAlias alias="memberVO" type="com.spring.collabee.biz.member.MemberVO"/>
   		<typeAlias alias="couponBoxVO" type="com.spring.collabee.biz.mycouponbox.MyCouponBoxVO"/>
   		<typeAlias alias="emoneyUsageVO" type="com.spring.collabee.biz.emoneyusage.EmoneyUsageVO"/>
		<typeAlias alias="pickListVO" type="com.spring.collabee.biz.mypicklist.MyPickListVO"/>
		<typeAlias alias="orderListVO" type="com.spring.collabee.biz.myorderlist.MyOrderListVO"/>
		<typeAlias alias="myReviewWrtVO"	type="com.spring.collabee.biz.myreview.MyReviewWrtVO"/>
		<typeAlias alias="proReviewVO"	type="com.spring.collabee.biz.myreview.ProReviewVO"/>
		<typeAlias alias="myproQnaVO"	type="com.spring.collabee.biz.proqna.MyProQnaVO"/>
  </typeAliases>
  

  
  
</configuration>